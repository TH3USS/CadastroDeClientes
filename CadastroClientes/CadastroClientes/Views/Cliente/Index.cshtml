@model IEnumerable<Cliente>

<h2>Lista de Clientes</h2>
<table>
    <tr>
        <th>Nome</th>
        <th>Email</th>
        <th>Telefone</th>
    </tr>
    @foreach (var cliente in Model)
    {
            <tr>
                <td>@cliente.Nome</td>
                <td>@cliente.Email</td>
                <td>@cliente.Telefone</td>
            </tr>
    }
</table>

<a href="/Cliente/Create">Cadastrar Novo Cliente</a>
